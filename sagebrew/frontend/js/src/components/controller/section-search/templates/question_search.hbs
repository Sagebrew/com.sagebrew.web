<div class="block sb_search_block">
    <div class="row">
        <div class="col-xs-4 col-sm-3">
            <div class="sb_search_vote_block">
                <h6>{{ vote_count }} Votes</h6>
                <h6>{{ view_count }} Views</h6>
            </div>
        </div>
        <div class="col-xs-8 sb_search_content_block">
            <h6><a href="{{ url }}">{{ title }}</a></h6>
            <p>{{ first_name }} {{ last_name }}</p>
            <small>{{ created }}</small>
        </div>
    </div>
</div>